      <CreatePreprocess3 NameAlgorithm="Null" BlExecute="True" Annotation="">
        <CellReference NameCell="Null" Pos="0" Type="" Annotation="" BlDisplay="False" NameCellComResult="Null" />
      </CreatePreprocess3>
      <CreatePreprocess4 NameAlgorithm="Null" BlExecute="True" Annotation="">
        <CellReference NameCell="Null" Pos="0" Type="" Annotation="" BlDisplay="False" NameCellComResult="Null" />
      </CreatePreprocess4>
      <CreatePreprocess5 NameAlgorithm="Null" BlExecute="True" Annotation="">
        <CellReference NameCell="Null" Pos="0" Type="" Annotation="" BlDisplay="False" NameCellComResult="Null" />
      </CreatePreprocess5>
    </CreateProcess>
    <Binary TypeParent="" TypeChild="" Pos="1" NoCamera="1" BlExecute="True">     
      <TypeBinary>Threshold</TypeBinary>
      <Thresh MinGray="0" MaxGray="240" />
      <Dyn MaskX="200" MaskY="200" Offset="25" TypeLightDyn_e="light" />
      <Dilation>0</Dilation>
      <OpenRadius>0</OpenRadius>
      <CloseRadius>5</CloseRadius>
      <MinArea>2000</MinArea>
      <MaxArea>99999999</MaxArea>
      <MinHeight>1</MinHeight>
      <MaxHeight>99999</MaxHeight>
      <MinWidth>1</MinWidth>
      <MaxWidth>99999</MaxWidth>
      <Fillup>True</Fillup>
      <BackgroundGray>0</BackgroundGray>
      <TypeOutBinary>Image</TypeOutBinary>
     
    </Binary>
  </Preprocess>

  <Output BlComprehensive="false">
    <SetResultValueCom >
      <ResultValueCom1 NameResult="" Expression="" Annotation=""/>
      <ResultValueCom2 NameResult="" Expression="" Annotation=""/>
      <ResultValueCom3 NameResult="" Expression="" Annotation=""/>
      <ResultValueCom4 NameResult="" Expression="" Annotation=""/>
      <ResultValueCom5 NameResult="" Expression="" Annotation=""/>
