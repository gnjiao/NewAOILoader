      <SelectAreaLow>50</SelectAreaLow>
      <ErrorAreaValue>2</ErrorAreaValue>
      <DilationDefectValue>3</DilationDefectValue>
      <GapIn>2</GapIn>
      <GapOut>34</GapOut>
    </Par>
  </Smooth>
  
