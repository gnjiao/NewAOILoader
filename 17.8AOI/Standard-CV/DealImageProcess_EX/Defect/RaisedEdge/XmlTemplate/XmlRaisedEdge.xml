      <CellReference NameCell="" Type="" Annotation=""/>
      <HObjectReference NameImage="" Function="" TypeHObject="" Annotation=""/>
    </HObjectReference>
  </ROI>

  <Std>
    <X></X>
    <Y></Y>
    <R></R>
    <R_J></R_J>
    <Num></Num>
    <Score></Score>
    <Scale></Scale>
  </Std>
  <Preprocess NameCell="C9" TypeParent="" TypeChild="" Pos="1" NoCamera="1" BlExecute="True">
    <CreateProcess NameCell="" TypeParent="" TypeChild="" Pos="0" NoCamera="1" BlExecute="False">
      <CreatePreprocess1 NameAlgorithm="二值化" BlExecute="True" Annotation="">
