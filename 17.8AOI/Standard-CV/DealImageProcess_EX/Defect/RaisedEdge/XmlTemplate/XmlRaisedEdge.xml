    <LogicBranch  NameCell="" TypeParent="" TypeChild="" Pos="">
      <Condition>
        <LogicCondition Condition=""/>
        <ExpressionA  Expression="" Result=""/>
        <ExpressionB  Expression="" Result=""/>
      </Condition>
    </LogicBranch>

    <OK>
